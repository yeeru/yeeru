#include<stdio.h>

int main(){
	
char Name[100];
int YYY[3],MM[2],DD[2];


do{
	fflush(stdin);
	printf("Please enter your English name:\n");
	scanf("%s",Name);
}while(strcmp(Name,"Terry") != 0);


do{
	fflush(stdin);
	printf("When is our first day..?(Hint:YYY MM DD)\n");
	scanf("%s""%s""%s",YYY,MM,DD);
}while(strcmp(YYY,"110") != 0 || strcmp(MM,"01") != 0 || strcmp(DD,"13") != 0);


printf("Right!\n");


return 0;

}
